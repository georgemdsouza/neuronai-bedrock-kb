# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# AWS Bedrock Agent (optional)
AWS_AGENT_ID=your_agent_id_here
AWS_AGENT_ALIAS_ID=your_agent_alias_id_here

# AI Model Configuration
AWS_MODEL=anthropic.claude-3-5-sonnet-20241022-v2:0
AWS_EMBEDDINGS_MODEL=amazon.titan-embed-text-v2:0

# Alternative: OpenAI Configuration
# OPENAI_API_KEY=your_openai_key_here
# OPENAI_MODEL=gpt-4
# OPENAI_EMBEDDINGS_MODEL=text-embedding-3-small
